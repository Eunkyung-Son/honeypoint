<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>		
		<typeAlias type="com.ek.honeypoint.models.Restaurant" alias="Restaurant"/>
		<typeAlias type="com.ek.honeypoint.models.Photofile" alias="Photofile"/>
		<typeAlias type="com.ek.honeypoint.models.RstrntMenu" alias="RstrntMenu"/>
		<typeAlias type="com.ek.honeypoint.models.Review" alias="Review"/>
		<typeAlias type="com.ek.honeypoint.models.ReviewCount" alias="ReviewCount"/>
		<typeAlias type="com.ek.honeypoint.models.ReviewImg" alias="ReviewImg"/>
		<typeAlias type="com.ek.honeypoint.models.InsertReviewImg" alias="InsertReviewImg"/>
		<typeAlias type="com.ek.honeypoint.models.UpdateReviewImg" alias="UpdateReviewImg"/>
		<typeAlias type="com.ek.honeypoint.models.Reservation" alias="Reservation"/>
		<typeAlias type="com.ek.honeypoint.models.Settle" alias="Settle"/>
		
		<!-- 멤버 -->
		<typeAlias type="com.ek.honeypoint.models.Member" alias="Member"/>
		<typeAlias type="com.ek.honeypoint.models.GnrlMember" alias="GnrlMember"/>
		<typeAlias type="com.ek.honeypoint.models.RestaurantMember" alias="RestaurantMember"/>
		<typeAlias type="com.ek.honeypoint.models.InsertResImg" alias="InsertResImg"/>
		<typeAlias type="com.ek.honeypoint.models.Menu" alias="Menu"/>
		<typeAlias type="com.ek.honeypoint.models.Photofile" alias="Photofile2"/>
		
		<!-- 찜 정보 -->
		<typeAlias type="com.ek.honeypoint.models.Favor" alias="Favor"/>
		
		<!-- 게시판 -->
		<typeAlias type="com.ek.honeypoint.models.Board" alias="Board"/>
		<typeAlias type="com.ek.honeypoint.models.SearchOption" alias="SearchOption"/>
		<typeAlias type="com.ek.honeypoint.models.Comment" alias="Comment"/>

	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/favor-mapper.xml"/>
		<mapper resource="mappers/review-mapper.xml"/>
		<mapper resource="mappers/restaurant-mapper.xml"/>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/board-mapper.xml"/>
	</mappers>
</configuration>
